{
  "name": "authenticator-browser-extension",
  "version": "0.0.0-development",
  "description": "Enables your browser-based automated tests to authenticate with your web app.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf target reports",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --ext ts --fix",
    "compile": "tsc --project tsconfig.json",
    "test:spec": "nyc --report-dir ./reports/coverage mocha --require ts-node/register --reporter spec 'spec/**/*.spec.ts'",
    "test:e2e:protractor": "protractor e2e/protractor/protractor.conf.js",
    "test:e2e:webdriverio": "wdio e2e/webdriverio/wdio.conf.js",
    "test": "npm run test:spec",
    "verify": "npm run clean && npm run lint && npm run test:spec && npm run compile && npm run test:e2e:protractor && npm run test:e2e:webdriverio",
    "publish:reports": "nyc report --reporter=text-lcov | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jan-molak/authenticator-browser-extension.git"
  },
  "keywords": [
    "chrome",
    "protractor",
    "webdriver",
    "wdio",
    "plugin",
    "extension"
  ],
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jan-molak/authenticator-browser-extension/issues"
  },
  "homepage": "https://github.com/jan-molak/authenticator-browser-extension#readme",
  "devDependencies": {
    "@serenity-js/assertions": "^2.3.1",
    "@serenity-js/core": "^2.3.1",
    "@serenity-js/local-server": "^2.3.1",
    "@serenity-js/protractor": "^2.3.1",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.5",
    "@types/mocha": "^7.0.2",
    "@types/mustache": "^4.0.1",
    "@types/semver": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@wdio/cli": "^6.0.14",
    "@wdio/local-runner": "^6.0.14",
    "@wdio/mocha-framework": "^6.0.13",
    "@wdio/spec-reporter": "^6.0.14",
    "chai": "^4.2.0",
    "chromedriver": "^80.0.2",
    "commitizen": "^4.0.4",
    "coveralls": "^3.0.11",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^7.0.0-alpha.3",
    "eslint-plugin-unicorn": "^18.0.1",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "mocha": "^7.1.1",
    "mocha-testdata": "^1.2.0",
    "nyc": "^15.0.1",
    "protractor": "^5.4.3",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.4",
    "semantic-release-cli": "^5.2.3",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "wdio-chromedriver-service": "^6.0.1"
  },
  "dependencies": {
    "mustache": "^4.0.1",
    "node-zip": "^1.1.1",
    "read-pkg": "^5.2.0",
    "semver": "^7.2.1",
    "tiny-types": "^1.12.4",
    "upath": "^1.2.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
